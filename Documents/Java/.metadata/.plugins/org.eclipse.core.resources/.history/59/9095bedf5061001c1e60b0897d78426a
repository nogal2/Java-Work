package main;

public class MainClass {

	public static void main(String[] args) {
		/*
			final 
			
			적용
			variable
			class
			method
		*/
		
		final int number = 10;	// final이 있으면 변수가 상수가 된다. 선언했으면 초기화 불가능
								// 대입 용도
	//	number = 20;	초기화 안됨
		
		final int MEMBERMAX = 20;	// final의 변수 이름은 다 대문자로 사용하는 경우가 많다.
		
		int num = number;
		int num1 = MEMBERMAX;
		
		if( num > 0 && num < MEMBERMAX) {
			
		}
		
	}

}

// class final	=> 상속금지
/*final*/ class Parent {	// 상속금지. 더 이상 계량하지말고 써라.
	public /*final*/ void method() {	// Over Ride 금지
		
	}
}

class Child extends Parent {
	
	public void method() {	// Over Ride
		
	}
}

