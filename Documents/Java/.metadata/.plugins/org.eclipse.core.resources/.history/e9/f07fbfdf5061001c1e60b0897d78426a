package cls;

public class YouClass {

	int number;
	
	public void memberMethod() {
		System.out.println("YouClass memberMethod");
	}
	
	public static void staticMethod() {		// 이 안에서 독립적으로 존재함. 그래서 멤버 변수는 사용할 수 없음.
		System.out.println("YouClass staticMethod");
		
	// 	사용 불가능
	//	number = 1;		멤버변수
	//	memberMethod();	일반 메소드
	//	this			this
	//	super			super
	}
	
	public static void swap(int arr[], int i, int j) {	//빠른 처리를 위해서 사용할 수 있다.
		int temp = arr[i];
		arr[i] = arr[j];
		arr[j] = temp;
	}
	
}
