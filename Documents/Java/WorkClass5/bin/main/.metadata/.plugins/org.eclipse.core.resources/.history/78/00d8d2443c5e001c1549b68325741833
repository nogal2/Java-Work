package cls;

public class Point3D {
	int x,y,z;
	
	Point3D(int x, int y, int z) {
		this.x=x;
		this.y=y;
		this.z=z; 
	}
	
	public Point3D() {
		this(0,0,0);
	}
	public boolean equals(Object obj) { /*
	(1) 인스턴스변수  x, y, z를  비교하도록  오버라이딩하시오. */
		if (x==y && y==z) {
			return true;
		} else {
			return false;
		}
	}
	public String toString() { /*
		(2) 인스턴스변수  x, y, z의  내용을  출력하도록  오버라이딩하시오. */
		return "["+x+","+y+","+z+"]";
	}
}
