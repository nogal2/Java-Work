package study;

import java.util.Scanner;

public class array3052 {

	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		int a[] = new int[10];
		for (int i = 0; i < a.length; i++) {
			a[i] = sc.nextInt();
		}
		int count = 0;
		for (int i = 0; i < a.length; i++) {
			for (int j = 1; j < a.length; j++) {
				if(a[i] == a[j]) {
					count++;
				}
			}
		}
		System.out.println(count);
		
	}

}
